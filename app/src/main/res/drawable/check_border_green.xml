<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
    <shape android:shape="rectangle">
    <stroke android:width="1px" android:color="#F1E3E3" />
    <corners android:radius="3dp" />
    <solid android:color="#B799A8F8" />
        <corners android:topLeftRadius="110dip"
            android:topRightRadius="110dip"
            android:bottomRightRadius="110dip"
            android:bottomLeftRadius="110dip" />
        <!--圆角矩形白色背景-->
    </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2px" android:color="#61000000" />
            <corners android:radius="10dp" />
            <solid android:color="#B7FFFFFF" />
            <corners android:topLeftRadius="120dip"
                android:topRightRadius="120dip"
                android:bottomRightRadius="120dip"
                android:bottomLeftRadius="120dip" />

        </shape>
    </item>
</selector>